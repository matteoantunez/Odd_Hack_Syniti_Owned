\b 6. \uc0\u1492 \u1490 \u1489 \u1500 \u1514  \u1495 \u1489 \u1493 \u1514 . 
\b0 \uc0\u1506 \u1491  \u1499 \u1502 \u1492  \u1513 \u1492 \u1491 \u1489 \u1512  \u1488 \u1497 \u1504 \u1493  \u1488 \u1505 \u1493 \u1512  \u1506 \u1500 -\u1508 \u1497  \u1495 \u1493 \u1511 , \u1489 \u1513 \u1493 \u1501  \u1502 \u1511 \u1512 \u1492  \u1500 \u1488  \u1514 \u1495 \u1493 \u1489  Apple \u1489 \u1490 \u1497 \u1503  \u1504 \u1494 \u1511  \u1490 \u1493 \u1508 \u1504 \u1497  \u1488 \u1493  \u1489 \u1490 \u1497 \u1503  \u1504 \u1494 \u1511 \u1497 \u1501  \u1504 \u1500 \u1493 \u1493 \u1497 \u1501 , \u1502 \u1497 \u1493 \u1495 \u1491 \u1497 \u1501 , \u1506 \u1511 \u1497 \u1508 \u1497 \u1501  \u1488 \u1493  \u1514 \u1493 \u1510 \u1488 \u1514 \u1497 \u1497 \u1501 , \u1500 \u1512 \u1489 \u1493 \u1514 , \u1488 \u1498  \u1500 \u1488  \u1512 \u1511 , \u1504 \u1494 \u1511 \u1497 \u1501  \u1489 \u1490 \u1497 \u1503  \u1488 \u1493 \u1489 \u1491 \u1503  \u1512 \u1493 \u1493 \u1495 \u1497 \u1501 , \u1492 \u1512 \u1505  \u1488 \u1493  \u1488 \u1493 \u1489 \u1491 \u1503  \u1504 \u1514 \u1493 \u1504 \u1497 \u1501  \u1488 \u1493  \u1502 \u1497 \u1491 \u1506 , \u1499 \u1513 \u1500  \u1489 \u1513 \u1500 \u1497 \u1495 \u1492  \u1488 \u1493  \u1511 \u1489 \u1500 \u1492  \u1513 \u1500  \u1504 \u1514 \u1493 \u1504 \u1497 \u1501  \u1499 \u1500 \u1513 \u1492 \u1501 , \u1492 \u1508 \u1512 \u1506 \u1492  \u1500 \u1502 \u1492 \u1500 \u1498  \u1506 \u1505 \u1511 \u1497 \u1501  \u1488 \u1493  \u1499 \u1500  \u1504 \u1494 \u1511  \u1488 \u1493  \u1488 \u1493 \u1489 \u1491 \u1503  \u1502 \u1505 \u1495 \u1512 \u1497  \u1488 \u1495 \u1512 , \u1492 \u1504 \u1493 \u1489 \u1506 \u1497 \u1501  \u1488 \u1493  \u1492 \u1511 \u1513 \u1493 \u1512 \u1497 \u1501  \u1500 \u1513 \u1497 \u1502 \u1493 \u1513 \u1498  \u1488 \u1493  \u1488 \u1497 -\u1497 \u1499 \u1493 \u1500 \u1514 \u1498  \u1500 \u1492 \u1513 \u1514 \u1502 \u1513  \u1489 \u1514 \u1493 \u1499 \u1504 \u1514  \u1492 -iPod \u1488 \u1493  \u1489 \u1506 \u1491 \u1499 \u1493 \u1504 \u1497 \u1501  \u1500 \u1514 \u1493 \u1499 \u1504 \u1514  \u1492 -iPod \u1488 \u1493  \u1489 \u1499 \u1500  \u1514 \u1493 \u1499 \u1504 \u1492  \u1488 \u1493  \u1497 \u1497 \u1513 \u1493 \u1501  \u1502 \u1510 \u1491  \u1513 \u1500 \u1497 \u1513 \u1497  \u1489 \u1497 \u1495 \u1491  \u1506 \u1501  \u1514 \u1493 \u1499 \u1504 \u1514  iPod \u1506 \u1501  \u1489 \u1506 \u1491 \u1499 \u1493 \u1504 \u1497 \u1501  \u1500 \u1514 \u1493 \u1499 \u1504 \u1514  \u1492 -iPod, \u1488 \u1513 \u1512  \u1497 \u1490 \u1512 \u1502 \u1493  \u1499 \u1499 \u1500  \u1513 \u1497 \u1490 \u1512 \u1502 \u1493 , \u1489 \u1492 \u1514 \u1506 \u1500 \u1501  \u1502 \u1514 \u1497 \u1488 \u1493 \u1512 \u1497 \u1497 \u1514  \u1492 \u1495 \u1489 \u1493 \u1514  (\u1495 \u1493 \u1494 \u1497 \u1514 , \u1504 \u1494 \u1497 \u1511 \u1497 \u1514  \u1488 \u1493  \u1488 \u1495 \u1512 \u1514 ), \u1493 \u1488 \u1507  \u1488 \u1501  Apple \u1497 \u1491 \u1506 \u1492  \u1506 \u1500  \u1492 \u1488 \u1508 \u1513 \u1512 \u1493 \u1514  \u1513 \u1500  \u1504 \u1494 \u1511 \u1497 \u1501  \u1499 \u1488 \u1502 \u1493 \u1512 . \u1497 \u1513 \u1504 \u1503  \u1502 \u1491 \u1497 \u1504 \u1493 \u1514  \u1488 \u1513 \u1512  \u1488 \u1497 \u1504 \u1503  \u1502 \u1514 \u1497 \u1512 \u1493 \u1514  \u1488 \u1497 -\u1492 \u1499 \u1500 \u1500 \u1492  \u1488 \u1493  \u1492 \u1490 \u1489 \u1500 \u1492  \u1513 \u1500  \u1495 \u1489 \u1493 \u1514  \u1489 \u1490 \u1497 \u1503  \u1504 \u1494 \u1511  \u1490 \u1493 \u1507 , \u1488 \u1493  \u1489 \u1490 \u1497 \u1503  \u1504 \u1494 \u1511 \u1497 \u1501  \u1504 \u1500 \u1493 \u1493 \u1497 \u1501  \u1488 \u1493  \u1514 \u1493 \u1510 \u1488 \u1514 \u1497 \u1497 \u1501 , \u1499 \u1498  \u1513 \u1497 \u1514 \u1499 \u1503  \u1513 \u1502 \u1490 \u1489 \u1500 \u1492  \u1494 \u1493  \u1488 \u1497 \u1504 \u1492  \u1495 \u1500 \u1492  \u1506 \u1500 \u1497 \u1498 . \u1489 \u1499 \u1500  \u1502 \u1511 \u1512 \u1492 , \u1495 \u1489 \u1493 \u1514 \u1492  \u1492 \u1499 \u1493 \u1500 \u1500 \u1514  \u1513 \u1500  Apple \u1499 \u1500 \u1508 \u1497 \u1498  \u1489 \u1490 \u1497 \u1503  \u1499 \u1500  \u1492 \u1504 \u1494 \u1511 \u1497 \u1501  (\u1500 \u1502 \u1506 \u1496  \u1499 \u1508 \u1497  \u1513 \u1497 \u1491 \u1512 \u1513  \u1500 \u1508 \u1497  \u1492 \u1495 \u1493 \u1511  \u1492 \u1512 \u1500 \u1493 \u1493 \u1504 \u1496 \u1497  \u1489 \u1502 \u1511 \u1512 \u1497 \u1501  \u1489 \u1492 \u1501  \u1502 \u1506 \u1493 \u1512 \u1489  \u1504 \u1494 \u1511  \u1490 \u1493 \u1507 ), \u1500 \u1488  \u1514 \u1506 \u1500 \u1492  \u1506 \u1500  \u1495 \u1502 \u1497 \u1513 \u1497 \u1501  \u1491 \u1493 \u1500 \u1512 \u1497 \u1501  \u1488 \u1502 \u1512 \u1497 \u1511 \u1488 \u1497 \u1497 \u1501  (U.S.$50.00). \u1492 \u1502 \u1490 \u1489 \u1500 \u1493 \u1514  \u1491 \u1500 \u1506 \u1497 \u1500  \u1497 \u1495 \u1493 \u1500 \u1493 , \u1488 \u1507  \u1488 \u1501  \u1492 \u1505 \u1506 \u1491  \u1499 \u1488 \u1502 \u1493 \u1512  \u1488 \u1497 \u1504 \u1493  \u1502 \u1511 \u1497 \u1497 \u1501  \u1488 \u1514  \u1502 \u1496 \u1512 \u1514 \u1493  \u1492 \u1502 \u1492 \u1493 \u1514 \u1497 \u1514 . \
