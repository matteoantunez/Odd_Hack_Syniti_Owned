\b 5. \uc0\u913 \u960 \u959 \u960 \u959 \u943 \u951 \u963 \u951  \u917 \u947 \u947 \u965 \u942 \u963 \u949 \u969 \u957 
\b0 .\
(\uc0\u945 ) \u917 \u940 \u957  \u949 \u943 \u963 \u964 \u949  \u960 \u949 \u955 \u940 \u964 \u951 \u962  \u960 \u959 \u965  \u949 \u943 \u957 \u945 \u953  \u954 \u945 \u964 \u945 \u957 \u945 \u955 \u969 \u964 \u942 \u962  (\u954 \u940 \u960 \u959 \u953 \u959 \u962  \u960 \u959 \u965  \u967 \u961 \u951 \u963 \u953 \u956 \u959 \u960 \u959 \u953 \u949 \u943  \u964 \u959  \u955 \u959 \u947 \u953 \u963 \u956 \u953 \u954 \u972  iPod \u949 \u954 \u964 \u972 \u962  \u964 \u951 \u962  \u949 \u961 \u947 \u945 \u963 \u943 \u945 \u962 , \u964 \u951 \u962  \u949 \u960 \u953 \u967 \u949 \u943 \u961 \u951 \u963 \u951 \u962  \u942  \u964 \u959 \u965  \u949 \u960 \u945 \u947 \u947 \u941 \u955 \u956 \u945 \u964 \u972 \u962  \u964 \u959 \u965 ), \u956 \u960 \u959 \u961 \u949 \u943  \u957 \u945  \u941 \u967 \u949 \u964 \u949  \u957 \u959 \u956 \u953 \u954 \u940  \u948 \u953 \u954 \u945 \u953 \u974 \u956 \u945 \u964 \u945  \u963 \u964 \u951  \u967 \u974 \u961 \u945  \u954 \u945 \u964 \u959 \u953 \u954 \u943 \u945 \u962  \u963 \u945 \u962  \u963 \u973 \u956 \u966 \u969 \u957 \u945  \u956 \u949  \u964 \u945  \u959 \u960 \u959 \u943 \u945  \u945 \u960 \u945 \u947 \u959 \u961 \u949 \u973 \u949 \u964 \u945 \u953  \u951  \u949 \u966 \u945 \u961 \u956 \u959 \u947 \u942  \u964 \u969 \u957  \u960 \u945 \u961 \u945 \u954 \u940 \u964 \u969  \u960 \u949 \u961 \u953 \u959 \u961 \u953 \u963 \u956 \u974 \u957  \u963 \u949  \u949 \u963 \u940 \u962 , \u954 \u945 \u953  \u972 \u960 \u959 \u965  \u953 \u963 \u967 \u973 \u949 \u953  \u945 \u965 \u964 \u942  \u951  \u945 \u960 \u945 \u947 \u972 \u961 \u949 \u965 \u963 \u951  \u959 \u953  \u960 \u949 \u961 \u953 \u959 \u961 \u953 \u963 \u956 \u959 \u943  \u945 \u965 \u964 \u959 \u943  \u948 \u949 \u957  \u952 \u945  \u953 \u963 \u967 \u973 \u959 \u965 \u957  \u963 \u949  \u949 \u963 \u940 \u962 . \u915 \u953 \u945  \u957 \u945  \u956 \u940 \u952 \u949 \u964 \u949  \u960 \u949 \u961 \u953 \u963 \u963 \u972 \u964 \u949 \u961 \u945  \u963 \u967 \u949 \u964 \u953 \u954 \u940  \u956 \u949  \u964 \u945  \u948 \u953 \u954 \u945 \u953 \u974 \u956 \u945 \u964 \u945 , \u949 \u960 \u953 \u954 \u959 \u953 \u957 \u969 \u957 \u942 \u963 \u964 \u949  \u956 \u949  \u941 \u957 \u945 \u957  \u964 \u959 \u960 \u953 \u954 \u972  \u959 \u961 \u947 \u945 \u957 \u953 \u963 \u956 \u972  \u949 \u957 \u951 \u956 \u941 \u961 \u969 \u963 \u951 \u962  \u960 \u949 \u955 \u945 \u964 \u974 \u957 .\
\
(\uc0\u946 ) \u913 \u925 \u913 \u915 \u925 \u937 \u929 \u921 \u918 \u917 \u932 \u917  \u929 \u919 \u932 \u913  \u922 \u913 \u921  \u931 \u933 \u924 \u934 \u937 \u925 \u917 \u921 \u932 \u917  \u931 \u932 \u927 \u925  \u914 \u913 \u920 \u924 \u927  \u928 \u927 \u933  \u917 \u928 \u921 \u932 \u929 \u917 \u928 \u917 \u932 \u913 \u921  \u913 \u928 \u927  \u932 \u927  \u917 \u934 \u913 \u929 \u924 \u927 \u931 \u932 \u917 \u927  \u916 \u921 \u922 \u913 \u921 \u927  \u927 \u932 \u921  \u919  \u935 \u929 \u919 \u931 \u919  \u932 \u927 \u933  \u923 \u927 \u915 \u921 \u931 \u924 \u921 \u922 \u927 \u933  iPOD \u922 \u913 \u921  \u932 \u937 \u925  \u917 \u925 \u919 \u924 \u917 \u929 \u937 \u931 \u917 \u937 \u925  \u923 \u927 \u915 \u921 \u931 \u924 \u921 \u922 \u927 \u933  iPOD \u915 \u921 \u925 \u917 \u932 \u913 \u921  \u913 \u928 \u927 \u922 \u923 \u917 \u921 \u931 \u932 \u921 \u922 \u913  \u933 \u928 ' \u917 \u933 \u920 \u933 \u925 \u919  \u931 \u913 \u931  \u922 \u913 \u921  \u927 \u932 \u921  \u927 \u923 \u927 \u922 \u923 \u919 \u929 \u927 \u931  \u927  \u922 \u921 \u925 \u916 \u933 \u925 \u927 \u931  \u915 \u921 \u913  \u932 \u919 \u925  \u921 \u922 \u913 \u925 \u927 \u928 \u927 \u921 \u919 \u932 \u921 \u922 \u919  \u928 \u927 \u921 \u927 \u932 \u919 \u932 \u913 , \u913 \u928 \u927 \u916 \u927 \u931 \u919 , \u913 \u922 \u929 \u921 \u914 \u917 \u921 \u913  \u922 \u913 \u921  \u913 \u928 \u927 \u928 \u917 \u921 \u929 \u913  \u914 \u913 \u929 \u933 \u925 \u917 \u921  \u917 \u931 \u913 \u931 .\
\
(\uc0\u947 ) \u931 \u932 \u927 \u925  \u924 \u917 \u915 \u921 \u931 \u932 \u927  \u914 \u913 \u920 \u924 \u927  \u928 \u927 \u933  \u917 \u928 \u921 \u932 \u929 \u917 \u928 \u917 \u932 \u913 \u921  \u913 \u928 \u927  \u932 \u919 \u925  \u921 \u931 \u935 \u933 \u927 \u933 \u931 \u913  \u925 \u927 \u924 \u927 \u920 \u917 \u931 \u921 \u913 , \u932 \u927  \u923 \u927 \u915 \u921 \u931 \u924 \u921 \u922 \u927  iPOD \u922 \u913 \u921  \u927 \u921  \u917 \u925 \u919 \u924 \u917 \u929 \u937 \u931 \u917 \u921 \u931  \u923 \u927 \u915 \u921 \u931 \u924 \u921 \u922 \u927 \u933  iPOD \u928 \u913 \u929 \u917 \u935 \u927 \u925 \u932 \u913 \u921  \'ab\u937 \u931  \u917 \u935 \u927 \u933 \u925 \'bb, \u924 \u917  \u927 \u923 \u913  \u932 \u913  \u917 \u923 \u913 \u932 \u932 \u937 \u924 \u913 \u932 \u913  \u922 \u913 \u921  \u935 \u937 \u929 \u921 \u931  \u922 \u913 \u924 \u921 \u913  \u917 \u915 \u915 \u933 \u919 \u931 \u919 , \u922 \u913 \u921  \u919  APPLE \u922 \u913 \u921  \u927 \u921  \u935 \u927 \u929 \u919 \u915 \u927 \u921  \u913 \u916 \u917 \u921 \u913 \u931  \u932 \u919 \u931  APPLE (\u931 \u933 \u923 \u923 \u927 \u915 \u921 \u922 \u913  \u913 \u925 \u913 \u934 \u917 \u929 \u927 \u924 \u917 \u925 \u927 \u921  \u937 \u931  \'abAPPLE\'bb \u915 \u921 \u913  \u932 \u927 \u933 \u931  \u931 \u922 \u927 \u928 \u927 \u933 \u931  \u932 \u937 \u925  \u917 \u925 \u927 \u932 \u919 \u932 \u937 \u925  5 \u922 \u913 \u921  6) \u916 \u921 \u913  \u932 \u927 \u933  \u928 \u913 \u929 \u927 \u925 \u932 \u927 \u931  \u913 \u928 \u927 \u928 \u927 \u921 \u927 \u933 \u925 \u932 \u913 \u921  \u927 \u923 \u917 \u931  \u932 \u921 \u931  \u917 \u915 \u915 \u933 \u919 \u931 \u917 \u921 \u931  \u922 \u913 \u921  \u931 \u933 \u925 \u920 \u919 \u922 \u917 \u931  \u931 \u917  \u931 \u935 \u917 \u931 \u919  \u924 \u917  \u932 \u927  \u923 \u927 \u915 \u921 \u931 \u924 \u921 \u922 \u927  iPOD \u922 \u913 \u921  \u932 \u921 \u931  \u917 \u925 \u919 \u924 \u917 \u929 \u937 \u931 \u917 \u921 \u931  \u923 \u927 \u915 \u921 \u931 \u924 \u921 \u922 \u927 \u933  iPOD, \u917 \u921 \u932 \u917  \u929 \u919 \u932 \u917 \u931 , \u931 \u933 \u925 \u917 \u928 \u913 \u915 \u927 \u924 \u917 \u925 \u917 \u931  \u905  \u920 \u917 \u931 \u924 \u921 \u922 \u917 \u931 , \u928 \u917 \u929 \u921 \u923 \u913 \u924 \u914 \u913 \u925 \u927 \u925 \u932 \u913 \u931  \u935 \u937 \u929 \u921 \u931  \u928 \u917 \u929 \u921 \u927 \u929 \u921 \u931 \u924 \u927 , \u932 \u921 \u931  \u931 \u933 \u925 \u917 \u928 \u913 \u915 \u927 \u924 \u917 \u925 \u917 \u931  \u917 \u915 \u915 \u933 \u919 \u931 \u917 \u921 \u931  \u905 /\u922 \u913 \u921  \u931 \u933 \u925 \u920 \u919 \u922 \u917 \u931  \u917 \u924 \u928 \u927 \u929 \u917 \u933 \u931 \u921 \u924 \u927 \u932 \u919 \u932 \u913 \u931 , \u921 \u922 \u913 \u925 \u927 \u928 \u927 \u921 \u919 \u932 \u921 \u922 \u919 \u931  \u928 \u927 \u921 \u927 \u932 \u919 \u932 \u913 \u931 , \u922 \u913 \u932 \u913 \u923 \u923 \u919 \u923 \u927 \u932 \u919 \u932 \u913 \u931  \u915 \u921 \u913  \u931 \u933 \u915 \u922 \u917 \u922 \u929 \u921 \u924 \u917 \u925 \u927  \u931 \u922 \u927 \u928 \u927 , \u913 \u922 \u929 \u921 \u914 \u917 \u921 \u913 \u931 , \u931 \u921 \u937 \u928 \u919 \u923 \u919 \u931  \u913 \u928 \u927 \u923 \u913 \u933 \u931 \u919 \u931  \u922 \u913 \u921  \u924 \u919  \u928 \u913 \u929 \u913 \u914 \u921 \u913 \u931 \u919 \u931  \u916 \u921 \u922 \u913 \u921 \u937 \u924 \u913 \u932 \u937 \u925  \u932 \u929 \u921 \u932 \u937 \u925 .\
\
(\uc0\u948 ) \u919  APPLE \u916 \u917 \u925  \u917 \u915 \u915 \u933 \u913 \u932 \u913 \u921  \u927 \u932 \u921  \u916 \u917 \u925  \u920 \u913  \u933 \u928 \u913 \u929 \u926 \u917 \u921  \u916 \u921 \u913 \u932 \u913 \u929 \u913 \u926 \u919  \u932 \u919 \u931  \u935 \u929 \u919 \u931 \u919 \u931  \u932 \u927 \u933  \u923 \u927 \u915 \u921 \u931 \u924 \u921 \u922 \u927 \u933  iPOD \u922 \u913 \u921  \u932 \u937 \u925  \u917 \u925 \u919 \u924 \u917 \u929 \u937 \u931 \u917 \u937 \u925  \u923 \u927 \u915 \u921 \u931 \u924 \u921 \u922 \u927 \u933  iPOD, \u927 \u932 \u921  \u927 \u921  \u923 \u917 \u921 \u932 \u927 \u933 \u929 \u915 \u921 \u917 \u931  \u928 \u927 \u933  \u928 \u917 \u929 \u921 \u923 \u913 \u924 \u914 \u913 \u925 \u927 \u925 \u932 \u913 \u921  \u931 \u932 \u927  \u923 \u927 \u915 \u921 \u931 \u924 \u921 \u922 \u927  iPOD \u922 \u913 \u921  \u931 \u932 \u921 \u931  \u917 \u925 \u919 \u924 \u917 \u929 \u937 \u931 \u917 \u921 \u931  \u923 \u927 \u915 \u921 \u931 \u924 \u921 \u922 \u927 \u933  iPOD \u920 \u913  \u913 \u925 \u932 \u913 \u928 \u927 \u922 \u929 \u921 \u920 \u927 \u933 \u925  \u931 \u932 \u921 \u931  \u913 \u928 \u913 \u921 \u932 \u919 \u931 \u917 \u921 \u931  \u931 \u913 \u931 , \u927 \u932 \u921  \u919  \u923 \u917 \u921 \u932 \u927 \u933 \u929 \u915 \u921 \u913  \u932 \u927 \u933  \u923 \u927 \u915 \u921 \u931 \u924 \u921 \u922 \u927 \u933  iPOD \u922 \u913 \u921  \u932 \u937 \u925  \u917 \u925 \u919 \u924 \u917 \u929 \u937 \u931 \u917 \u937 \u925  \u923 \u927 \u915 \u921 \u931 \u924 \u921 \u922 \u927 \u933  iPOD \u920 \u913  \u917 \u921 \u925 \u913 \u921  \u913 \u916 \u921 \u913 \u923 \u917 \u921 \u928 \u932 \u919  \u905  \u935 \u937 \u929 \u921 \u931  \u931 \u934 \u913 \u923 \u924 \u913 \u932 \u913 , \u927 \u932 \u921  \u932 \u927  \u923 \u927 \u915 \u921 \u931 \u924 \u921 \u922 \u927  iPOD \u922 \u913 \u921  \u927 \u921  \u917 \u925 \u919 \u924 \u917 \u929 \u937 \u931 \u917 \u921 \u931  \u923 \u927 \u915 \u921 \u931 \u924 \u921 \u922 \u927 \u933  iPOD \u920 \u913  \u917 \u921 \u925 \u913 \u921  \u931 \u933 \u924 \u914 \u913 \u932 \u913  \u905  \u920 \u913  \u931 \u933 \u925 \u917 \u929 \u915 \u913 \u918 \u927 \u925 \u932 \u913 \u921  \u924 \u917  \u923 \u927 \u915 \u921 \u931 \u924 \u921 \u922 \u927 , \u917 \u934 \u913 \u929 \u924 \u927 \u915 \u917 \u931  \u905  \u933 \u928 \u919 \u929 \u917 \u931 \u921 \u917 \u931  \u932 \u929 \u921 \u932 \u937 \u925  \u905  \u927 \u932 \u921  \u932 \u933 \u935 \u927 \u925  \u931 \u934 \u913 \u923 \u924 \u913 \u932 \u913  \u931 \u932 \u927  \u923 \u927 \u915 \u921 \u931 \u924 \u921 \u922 \u927  iPOD \u922 \u913 \u921  \u931 \u932 \u921 \u931  \u917 \u925 \u919 \u924 \u917 \u929 \u937 \u931 \u917 \u921 \u931  \u923 \u927 \u915 \u921 \u931 \u924 \u921 \u922 \u927 \u933  iPOD \u920 \u913  \u917 \u928 \u921 \u916 \u921 \u927 \u929 \u920 \u937 \u920 \u927 \u933 \u925 . \u919  \u917 \u915 \u922 \u913 \u932 \u913 \u931 \u932 \u913 \u931 \u919  \u913 \u933 \u932 \u927 \u933  \u932 \u927 \u933  \u923 \u927 \u915 \u921 \u931 \u924 \u921 \u922 \u927 \u933  \u917 \u925 \u916 \u917 \u935 \u917 \u932 \u913 \u921  \u925 \u913  \u917 \u928 \u919 \u929 \u917 \u913 \u931 \u917 \u921  \u932 \u919  \u935 \u929 \u919 \u931 \u932 \u921 \u922 \u927 \u932 \u919 \u932 \u913  \u932 \u927 \u933  \u923 \u927 \u915 \u921 \u931 \u924 \u921 \u922 \u927 \u933 , \u917 \u934 \u913 \u929 \u924 \u927 \u915 \u937 \u925  \u905  \u933 \u928 \u919 \u929 \u917 \u931 \u921 \u937 \u925  \u932 \u929 \u921 \u932 \u937 \u925 .\
\
(\uc0\u949 ) \u917 \u928 \u921 \u928 \u929 \u927 \u931 \u920 \u917 \u932 \u937 \u931 , \u913 \u925 \u913 \u915 \u925 \u937 \u929 \u921 \u918 \u917 \u932 \u917  \u927 \u932 \u921  \u932 \u927  \u923 \u927 \u915 \u921 \u931 \u924 \u921 \u922 \u927  iPOD \u922 \u913 \u921  \u927 \u921  \u917 \u925 \u919 \u924 \u917 \u929 \u937 \u931 \u917 \u921 \u931  \u923 \u927 \u915 \u921 \u931 \u924 \u921 \u922 \u927 \u933  iPOD \u916 \u917 \u925  \u928 \u929 \u927 \u927 \u929 \u921 \u918 \u927 \u925 \u932 \u913 \u921  \u927 \u933 \u932 \u917  \u917 \u921 \u925 \u913 \u921  \u922 \u913 \u932 \u913 \u923 \u923 \u919 \u923 \u917 \u931  \u915 \u921 \u913  \u935 \u929 \u919 \u931 \u919  \u931 \u917  \u928 \u917 \u929 \u921 \u931 \u932 \u913 \u931 \u917 \u921 \u931  \u905  \u928 \u917 \u929 \u921 \u914 \u913 \u923 \u923 \u927 \u925 \u932 \u913  \u927 \u928 \u927 \u933  \u919  \u913 \u925 \u917 \u928 \u913 \u929 \u922 \u917 \u921 \u913  \u905  \u927 \u921  \u922 \u913 \u920 \u933 \u931 \u932 \u917 \u929 \u919 \u931 \u917 \u921 \u931 , \u932 \u913  \u931 \u934 \u913 \u923 \u924 \u913 \u932 \u913  \u905  \u927 \u921  \u913 \u925 \u913 \u922 \u929 \u921 \u914 \u917 \u921 \u917 \u931  \u931 \u932 \u927  \u928 \u917 \u929 \u921 \u917 \u935 \u927 \u924 \u917 \u925 \u927 , \u932 \u913  \u916 \u917 \u916 \u927 \u924 \u917 \u925 \u913  \u905  \u932 \u921 \u931  \u928 \u923 \u919 \u929 \u927 \u934 \u927 \u929 \u921 \u917 \u931  \u928 \u927 \u933  \u928 \u913 \u929 \u917 \u935 \u927 \u925 \u932 \u913 \u921  \u913 \u928 \u927  \u932 \u927  \u923 \u927 \u915 \u921 \u931 \u924 \u921 \u922 \u927  iPOD \u922 \u913 \u921  \u932 \u921 \u931  \u917 \u925 \u919 \u924 \u917 \u929 \u937 \u931 \u917 \u921 \u931  \u923 \u927 \u915 \u921 \u931 \u924 \u921 \u922 \u927 \u933  iPOD \u917 \u925 \u916 \u917 \u935 \u917 \u932 \u913 \u921  \u925 \u913  \u917 \u935 \u927 \u933 \u925  \u937 \u931  \u931 \u933 \u925 \u917 \u928 \u917 \u921 \u913  \u932 \u927 \u925  \u920 \u913 \u925 \u913 \u932 \u927 , \u931 \u937 \u924 \u913 \u932 \u921 \u922 \u917 \u931  \u914 \u923 \u913 \u914 \u917 \u931  \u905  \u931 \u927 \u914 \u913 \u929 \u917 \u931  \u933 \u923 \u921 \u922 \u917 \u931  \u918 \u919 \u924 \u921 \u917 \u931  \u905  \u914 \u923 \u913 \u914 \u919  \u931 \u932 \u927  \u928 \u917 \u929 \u921 \u914 \u913 \u923 \u923 \u927 \u925 , \u931 \u933 \u924 \u928 \u917 \u929 \u921 \u923 \u913 \u924 \u914 \u913 \u925 \u927 \u924 \u917 \u925 \u937 \u925 , \u924 \u917 \u932 \u913 \u926 \u933  \u913 \u923 \u923 \u937 \u925 , \u932 \u919 \u931  \u917 \u928 \u921 \u935 \u917 \u921 \u929 \u919 \u931 \u921 \u913 \u922 \u919 \u931  \u923 \u917 \u921 \u932 \u927 \u933 \u929 \u915 \u921 \u913 \u931  \u928 \u933 \u929 \u919 \u925 \u921 \u922 \u937 \u925  \u917 \u915 \u922 \u913 \u932 \u913 \u931 \u932 \u913 \u931 \u917 \u937 \u925 , \u931 \u933 \u931 \u932 \u919 \u924 \u913 \u932 \u937 \u925  \u928 \u923 \u927 \u919 \u915 \u919 \u931 \u919 \u931  \u905  \u917 \u928 \u921 \u922 \u927 \u921 \u925 \u937 \u925 \u921 \u913 \u931  \u913 \u917 \u929 \u927 \u931 \u922 \u913 \u934 \u937 \u925 , \u917 \u923 \u917 \u915 \u935 \u927 \u933  \u917 \u925 \u913 \u917 \u929 \u921 \u913 \u931  \u922 \u933 \u922 \u923 \u927 \u934 \u927 \u929 \u921 \u913 \u931 , \u933 \u928 \u927 \u931 \u932 \u919 \u929 \u921 \u926 \u919 \u931  \u918 \u937 \u932 \u921 \u922 \u937 \u925  \u923 \u917 \u921 \u932 \u927 \u933 \u929 \u915 \u921 \u937 \u925  \u905  \u927 \u928 \u923 \u921 \u922 \u937 \u925  \u931 \u933 \u931 \u932 \u919 \u924 \u913 \u932 \u937 \u925 .\
