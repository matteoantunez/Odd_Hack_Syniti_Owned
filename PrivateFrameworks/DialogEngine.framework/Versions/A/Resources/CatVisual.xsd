
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:annotation>
    <xsd:documentation xml:lang="en">
      Visual CAT schema
      Copyright (c) 2021 Apple. All rights reserved.
    </xsd:documentation>
  </xsd:annotation>

  <xsd:element name="cat">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="meta" minOccurs="0" maxOccurs="unbounded">
          <xsd:complexType>
            <xsd:attribute name="name" type="xsd:string"/>
            <xsd:attribute name="value" type="xsd:string"/>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="parameters" minOccurs="0">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="parameter" maxOccurs="unbounded" type="Parameter"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="conditions" minOccurs="0">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="condition" maxOccurs="unbounded" type="Condition"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="phrases" minOccurs="0">
          <xsd:complexType>
            <xsd:sequence maxOccurs="unbounded">
              <xsd:element name="random" type="Random" minOccurs="0"/>
              <xsd:element name="first" type="First" minOccurs="0"/>
              <xsd:element name="switch" type="Switch" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="response" type="SnippetUI.Response"/>
      </xsd:sequence>
<!-- lxml BAD: does not allow attribute with name "xmlns"
      <xsd:attribute name="xmlns" type="string" use="required"/>
-->
      <xsd:attribute name="id" type="xsd:string" use="required"/>
    </xsd:complexType>
  </xsd:element>

  <xsd:complexType name="Parameter">
    <xsd:attribute name="name" type="xsd:string" use="required"/>
    <xsd:attribute name="type" type="xsd:string" use="required"/>
    <xsd:attribute name="description" type="xsd:string"/>
    <xsd:attribute name="semanticConcept" type="xsd:string"/>
    <xsd:attribute name="sampleValue" type="xsd:string"/>
    <xsd:attribute name="deprecated" type="xsd:string"/>
    <xsd:attribute name="profanityFilter" type="xsd:string"/>
    <xsd:attribute name="optional" type="xsd:string"/>
    <xsd:attribute name="valueFromCat" type="xsd:string"/>
  </xsd:complexType>

  <!-- conditions are named boolean expressions -->
  <xsd:group name="condition_elements">
    <xsd:sequence>
      <xsd:element name="expression" type="Expression" minOccurs="0"/>
      <xsd:element name="sub-condition" type="SubCondition" minOccurs="0"/>
      <xsd:element name="and" type="And" minOccurs="0"/>
      <xsd:element name="or" type="Or" minOccurs="0"/>
      <xsd:element name="not" type="Not" minOccurs="0"/>
    </xsd:sequence>
  </xsd:group>

  <xsd:complexType name="Condition">
    <xsd:group ref="condition_elements" minOccurs="0" maxOccurs="1"/>
    <xsd:attribute name="name" type="xsd:string" use="required"/>
    <xsd:attribute name="note" type="xsd:string"/>
  </xsd:complexType>

  <!-- a single expression within a condition -->
  <xsd:complexType name="Expression">
    <xsd:attribute name="name" type="xsd:string" use="required"/>
    <xsd:attribute name="op" type="xsd:string"/>
    <xsd:attribute name="value" type="xsd:string"/>
    <xsd:attribute name="note" type="xsd:string"/>
  </xsd:complexType>

  <!-- a nested sub condition within a condition -->
  <xsd:complexType name="SubCondition">
    <xsd:attribute name="name" type="xsd:string" use="required"/>
    <xsd:attribute name="note" type="xsd:string"/>
  </xsd:complexType>

  <!-- the set of boolean operators we support -->
  <xsd:complexType name="And">
    <xsd:group ref="condition_elements" minOccurs="0" maxOccurs="unbounded"/>
    <xsd:attribute name="note" type="xsd:string"/>
  </xsd:complexType>
  <xsd:complexType name="Or">
    <xsd:group ref="condition_elements" minOccurs="0" maxOccurs="unbounded"/>
    <xsd:attribute name="note" type="xsd:string"/>
  </xsd:complexType>
  <xsd:complexType name="Not">
    <xsd:group ref="condition_elements" minOccurs="0" maxOccurs="unbounded"/>
    <xsd:attribute name="note" type="xsd:string"/>
  </xsd:complexType>

  <!-- phrases are embeddable and reusable pieces of text -->
  <xsd:group name="grouping_elements">
    <xsd:sequence>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element name="random" type="Random"/>
        <xsd:element name="first" type="First"/>
        <xsd:element name="switch" type="Switch"/>
        <xsd:element name="all" type="All"/>
        <xsd:element name="text" type="PhraseGroupText"/>
      </xsd:choice>
    </xsd:sequence>
  </xsd:group>

  <xsd:attributeGroup name="group_elem_attrs">
    <xsd:attribute name="id" type="xsd:string"/>
    <xsd:attribute name="condition" type="xsd:string"/>
    <xsd:attribute name="phrase" type="xsd:string"/>
    <xsd:attribute name="case" type="xsd:string"/>
    <xsd:attribute name="weight" type="xsd:string"/>
    <xsd:attribute name="note" type="xsd:string"/>
  </xsd:attributeGroup>

  <xsd:complexType name="Random">
    <xsd:group ref="grouping_elements"/>
    <xsd:attributeGroup ref="group_elem_attrs"/>
  </xsd:complexType>

  <xsd:complexType name="First">
    <xsd:group ref="grouping_elements"/>
    <xsd:attributeGroup ref="group_elem_attrs"/>
  </xsd:complexType>

  <xsd:complexType name="Switch">
    <xsd:group ref="grouping_elements"/>
    <xsd:attributeGroup ref="group_elem_attrs"/>
  </xsd:complexType>

  <xsd:complexType name="All">
    <xsd:group ref="grouping_elements"/>
    <xsd:attributeGroup ref="group_elem_attrs"/>
  </xsd:complexType>

  <!-- CAT text element, not to be confused with SnippetUI text element -->
  <xsd:complexType name="PhraseGroupText" mixed="true">
    <xsd:sequence>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:group ref="grouping_elements"/>
        <xsd:group ref="dialog_elements"/>
      </xsd:choice>
    </xsd:sequence>
    <xsd:attribute name="id" type="xsd:string"/>
    <xsd:attribute name="condition" type="xsd:string"/>
    <xsd:attribute name="case" type="xsd:string"/>
    <xsd:attribute name="weight" type="xsd:string"/>
    <xsd:attribute name="note" type="xsd:string"/>
  </xsd:complexType>

  <!-- CAT elements used as children of SnippetUI leaf elements -->
  <xsd:group name="dialog_elements">
    <xsd:sequence>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element name="var" type="Var"/>
        <xsd:element name="phrase" type="Phrase"/>
        <xsd:element name="opt" type="Opt"/>
        <xsd:element name="transform" type="Transform"/>
      </xsd:choice>
    </xsd:sequence>
  </xsd:group>

  <!-- the var element supports variable substitution -->
  <xsd:complexType name="Var">
    <xsd:attribute name="name" type="xsd:string"/>
    <xsd:attribute name="inflect" type="xsd:string"/>
    <xsd:attribute name="transform" type="xsd:string"/>
    <xsd:attribute name="number" type="xsd:string"/>
    <xsd:attribute name="list" type="xsd:string"/>
    <xsd:attribute name="separator" type="xsd:string"/>
    <xsd:attribute name="beforefirst" type="xsd:string"/>
    <xsd:attribute name="afterfirst" type="xsd:string"/>
    <xsd:attribute name="beforelast" type="xsd:string"/>
    <xsd:attribute name="afterlast" type="xsd:string"/>
    <xsd:attribute name="itemprefix" type="xsd:string"/>
    <xsd:attribute name="itemsuffix" type="xsd:string"/>
    <xsd:attribute name="itemformat" type="xsd:string"/>
    <xsd:attribute name="ground" type="xsd:string"/>
    <xsd:attribute name="groundvar" type="xsd:string"/>
    <xsd:attribute name="semanticConcept" type="xsd:string"/>
    <xsd:attribute name="id" type="xsd:string"/>
  </xsd:complexType>

  <!-- the phrase element lets you embed other segments of text -->
  <xsd:complexType name="Phrase">
    <xsd:attribute name="name" type="xsd:string"/>
    <xsd:attribute name="id" type="xsd:string"/>
  </xsd:complexType>

  <!-- opt defines optional output if variable substitutions were empty -->
  <xsd:complexType name="Opt" mixed="true">
    <xsd:group ref="dialog_elements"/>
  </xsd:complexType>

  <!-- transform the child text in various ways, e.g., uppercase -->
  <xsd:complexType name="Transform" mixed="true">
    <xsd:group ref="dialog_elements"/>
    <xsd:attribute name="mode" type="xsd:string" use="required"/>
  </xsd:complexType>

  <!-- elements with string values, dialog_elements, and/or condition attributes -->
  <xsd:complexType name="DialogElements" mixed="true">
    <xsd:group ref="dialog_elements"/>
  </xsd:complexType>

  <xsd:complexType name="DialogElements.Repeated" mixed="true">
    <xsd:group ref="dialog_elements"/>
    <xsd:attribute name="list" type="xsd:string"/>
    <xsd:attribute name="iterator" type="xsd:string"/>
  </xsd:complexType>

  <xsd:complexType name="DialogElements.WithCondition" mixed="true">
    <xsd:group ref="dialog_elements"/>
    <xsd:attribute name="condition" type="xsd:string"/>
  </xsd:complexType>

  <xsd:complexType name="DialogElements.Repeated.WithCondition" mixed="true">
    <xsd:group ref="dialog_elements"/>
    <xsd:attribute name="list" type="xsd:string"/>
    <xsd:attribute name="iterator" type="xsd:string"/>
    <xsd:attribute name="condition" type="xsd:string"/>
  </xsd:complexType>

  <xsd:complexType name="String.Repeated" mixed="true">
    <xsd:attribute name="list" type="xsd:string"/>
    <xsd:attribute name="iterator" type="xsd:string"/>
  </xsd:complexType>

  <xsd:complexType name="String.WithCondition" mixed="true">
    <xsd:attribute name="condition" type="xsd:string"/>
  </xsd:complexType>

  <xsd:complexType name="String.Repeated.WithCondition" mixed="true">
    <xsd:attribute name="list" type="xsd:string"/>
    <xsd:attribute name="iterator" type="xsd:string"/>
    <xsd:attribute name="condition" type="xsd:string"/>
  </xsd:complexType>

  <!-- visual elements for the SnippetUI protobuffer data model -->

  <xsd:complexType name="SnippetUI.ActionElement">
    <xsd:sequence>
      <xsd:element name="command" type="SnippetUI.Command" minOccurs="0"/>
      <xsd:element name="text-elements" type="SnippetUI.TextElement" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="idioms" type="SnippetUI.Idiom" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
    <xsd:attribute name="list" type="xsd:string"/>
    <xsd:attribute name="iterator" type="xsd:string"/>
  </xsd:complexType>

  <xsd:complexType name="SnippetUI.ActionProperty">
    <xsd:sequence>
      <xsd:element name="actions" type="SnippetUI.ActionElement" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
    <xsd:attribute name="condition" type="xsd:string"/>
  </xsd:complexType>

  <xsd:complexType name="SnippetUI.BackgroundElement">
    <xsd:sequence>
      <xsd:element name="visual" type="SnippetUI.VisualElement" minOccurs="0"/>
      <xsd:element name="idioms" type="SnippetUI.Idiom" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="is-hidden-on-idioms" type="DialogElements" minOccurs="0"/>
    </xsd:sequence>
    <xsd:attribute name="list" type="xsd:string"/>
    <xsd:attribute name="iterator" type="xsd:string"/>
    <xsd:attribute name="condition" type="xsd:string"/>
  </xsd:complexType>

  <xsd:complexType name="SnippetUI.BinaryButton">
    <xsd:sequence>
      <xsd:element name="primary-button" type="SnippetUI.Button" minOccurs="0"/>
      <xsd:element name="secondary-button" type="SnippetUI.Button" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="SnippetUI.Button">
    <xsd:sequence>
      <xsd:element name="action" type="SnippetUI.ActionProperty" minOccurs="0"/>
      <xsd:element name="label" type="SnippetUI.TextProperty" minOccurs="0"/>
      <xsd:element name="thumbnail" type="SnippetUI.VisualProperty" minOccurs="0"/>
      <xsd:element name="form-action" type="SnippetUI.Button.FormAction" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="SnippetUI.Button_Container">
    <xsd:sequence>
      <xsd:element name="button-items" type="SnippetUI.Button_Container.ButtonItem" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="SnippetUI.Button_Container.ButtonItem">
    <xsd:sequence>
      <xsd:element name="value" minOccurs="0">
        <xsd:complexType>
          <xsd:choice>
            <xsd:element name="button" type="SnippetUI.Button"/>
            <xsd:element name="player-button" type="SnippetUI.PlayerButton"/>
          </xsd:choice>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="list" type="xsd:string"/>
    <xsd:attribute name="iterator" type="xsd:string"/>
  </xsd:complexType>

  <xsd:complexType name="SnippetUI.Color">
    <xsd:sequence>
      <xsd:element name="value" minOccurs="0">
        <xsd:complexType>
          <xsd:choice>
            <xsd:element name="hex-value" type="SnippetUI.Color.HexValue"/>
            <xsd:element name="name" type="SnippetUI.Color.Name"/>
          </xsd:choice>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="SnippetUI.Color.HexValue">
    <xsd:sequence>
      <xsd:element name="rgba" type="DialogElements" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="SnippetUI.ColorElement">
    <xsd:sequence>
      <xsd:element name="color" type="SnippetUI.Color" minOccurs="0"/>
      <xsd:element name="idioms" type="SnippetUI.Idiom" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="SnippetUI.Command">
    <xsd:sequence>
      <xsd:element name="is-navigation" type="DialogElements" minOccurs="0"/>
      <xsd:element name="value" minOccurs="0">
        <xsd:complexType>
          <xsd:choice>
            <xsd:element name="no-op" type="DialogElements.WithCondition"/>
            <xsd:element name="ace-command" type="DialogElements.WithCondition"/>
            <xsd:element name="response" type="DialogElements.WithCondition"/>
            <xsd:element name="utterance" type="DialogElements.WithCondition"/>
            <xsd:element name="change-background" type="SnippetUI.Command.ChangeBackground"/>
            <xsd:element name="sf-command" type="DialogElements.WithCondition"/>
            <xsd:element name="direct-invocation" type="DialogElements.WithCondition"/>
            <xsd:element name="response-group" type="SnippetUI.Command.ResponseGroup"/>
          </xsd:choice>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="SnippetUI.Command.ChangeBackground">
    <xsd:sequence>
      <xsd:element name="background" type="SnippetUI.BackgroundElement" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="SnippetUI.Command.ResponseGroup">
    <xsd:sequence>
      <xsd:element name="group-id" type="xsd:string" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="SnippetUI.Control_Slider">
    <xsd:sequence>
      <xsd:element name="slider" type="SnippetUI.Slider" minOccurs="0"/>
      <xsd:element name="text-1" type="SnippetUI.TextProperty" minOccurs="0"/>
      <xsd:element name="action" type="SnippetUI.ActionProperty" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="SnippetUI.Control_Switch">
    <xsd:sequence>
      <xsd:element name="switch" type="SnippetUI.Switch" minOccurs="0"/>
      <xsd:element name="text-1" type="SnippetUI.TextProperty" minOccurs="0"/>
      <xsd:element name="text-2" type="SnippetUI.TextProperty" minOccurs="0"/>
      <xsd:element name="thumbnail" type="SnippetUI.VisualProperty" minOccurs="0"/>
      <xsd:element name="action" type="SnippetUI.ActionProperty" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="SnippetUI.CoreChart">
    <xsd:sequence>
      <xsd:element name="data" type="DialogElements.WithCondition" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="SnippetUI.CoreChartElement">
    <xsd:sequence>
      <xsd:element name="chart" type="SnippetUI.CoreChart" minOccurs="0"/>
      <xsd:element name="idioms" type="SnippetUI.Idiom" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="SnippetUI.CustomCanvas">
    <xsd:sequence>
      <xsd:element name="bundle-name" type="DialogElements" minOccurs="0"/>
      <xsd:element name="view-id" type="DialogElements" minOccurs="0"/>
      <xsd:element name="view-data" type="DialogElements" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="SnippetUI.CustomElement">
    <xsd:sequence>
      <xsd:element name="canvas" type="SnippetUI.CustomCanvas" minOccurs="0"/>
      <xsd:element name="idioms" type="SnippetUI.Idiom" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="SnippetUI.FactItem_Button">
    <xsd:sequence>
      <xsd:element name="text-1" type="SnippetUI.TextProperty" minOccurs="0"/>
      <xsd:element name="button-1" type="SnippetUI.Button" minOccurs="0"/>
      <xsd:element name="button-2" type="SnippetUI.Button" minOccurs="0"/>
      <xsd:element name="button-3" type="SnippetUI.Button" minOccurs="0"/>
      <xsd:element name="text-2" type="SnippetUI.TextProperty" minOccurs="0"/>
      <xsd:element name="text-3" type="SnippetUI.TextProperty" minOccurs="0"/>
      <xsd:element name="text-4" type="SnippetUI.MultilineTextProperty" minOccurs="0"/>
      <xsd:element name="thumbnail" type="SnippetUI.VisualProperty" minOccurs="0"/>
      <xsd:element name="action" type="SnippetUI.ActionProperty" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="SnippetUI.FactItem_HeroNumber">
    <xsd:sequence>
      <xsd:element name="number" type="SnippetUI.TextProperty" minOccurs="0"/>
      <xsd:element name="text-1" type="SnippetUI.TextProperty" minOccurs="0"/>
      <xsd:element name="text-2" type="SnippetUI.MultilineTextProperty" minOccurs="0"/>
      <xsd:element name="text-3" type="SnippetUI.MultilineTextProperty" minOccurs="0"/>
      <xsd:element name="action" type="SnippetUI.ActionProperty" minOccurs="0"/>
      <xsd:element name="text-4" type="SnippetUI.TextProperty" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="SnippetUI.FactItem_ShortNumber">
    <xsd:sequence>
      <xsd:element name="number" type="SnippetUI.TextProperty" minOccurs="0"/>
      <xsd:element name="text-1" type="SnippetUI.TextProperty" minOccurs="0"/>
      <xsd:element name="text-2" type="SnippetUI.MultilineTextProperty" minOccurs="0"/>
      <xsd:element name="text-3" type="SnippetUI.MultilineTextProperty" minOccurs="0"/>
      <xsd:element name="unit" type="SnippetUI.TextProperty" minOccurs="0"/>
      <xsd:element name="thumbnail" type="SnippetUI.VisualProperty" minOccurs="0"/>
      <xsd:element name="action" type="SnippetUI.ActionProperty" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="SnippetUI.FactItem_Standard">
    <xsd:sequence>
      <xsd:element name="text-1" type="SnippetUI.TextProperty" minOccurs="0"/>
      <xsd:element name="text-2" type="SnippetUI.TextProperty" minOccurs="0"/>
      <xsd:element name="text-3" type="SnippetUI.TextProperty" minOccurs="0"/>
      <xsd:element name="text-4" type="SnippetUI.MultilineTextProperty" minOccurs="0"/>
      <xsd:element name="text-5" type="SnippetUI.TextProperty" minOccurs="0"/>
      <xsd:element name="thumbnail" type="SnippetUI.VisualProperty" minOccurs="0"/>
      <xsd:element name="action" type="SnippetUI.ActionProperty" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="SnippetUI.HeroHeader">
    <xsd:sequence>
      <xsd:element name="banner" type="SnippetUI.VisualProperty" minOccurs="0"/>
      <xsd:element name="text-1" type="SnippetUI.TextProperty" minOccurs="0"/>
      <xsd:element name="text-2" type="SnippetUI.TextProperty" minOccurs="0"/>
      <xsd:element name="watch-list-button" type="SnippetUI.WatchListButton" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="SnippetUI.HorizontalList_Standard">
    <xsd:sequence>
      <xsd:element name="items" type="SnippetUI.HorizontalList_Standard.Item" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="text-1" type="SnippetUI.TextProperty" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="SnippetUI.HorizontalList_Standard.Item">
    <xsd:sequence>
      <xsd:element name="style" type="SnippetUI.HorizontalList_Standard.Item.Style" minOccurs="0"/>
      <xsd:element name="thumbnail" type="SnippetUI.VisualProperty" minOccurs="0"/>
      <xsd:element name="text-1" type="SnippetUI.TextProperty" minOccurs="0"/>
      <xsd:element name="text-2" type="SnippetUI.TextProperty" minOccurs="0"/>
      <xsd:element name="action" type="SnippetUI.ActionProperty" minOccurs="0"/>
    </xsd:sequence>
    <xsd:attribute name="list" type="xsd:string"/>
    <xsd:attribute name="iterator" type="xsd:string"/>
  </xsd:complexType>

  <xsd:complexType name="SnippetUI.ImageElement">
    <xsd:sequence>
      <xsd:element name="sources" type="SnippetUI.ImageElement.Source" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="idioms" type="SnippetUI.Idiom" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="SnippetUI.ImageElement.AppIcon">
    <xsd:sequence>
      <xsd:element name="application-bundle-identifier" type="DialogElements" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="SnippetUI.ImageElement.Bundle">
    <xsd:sequence>
      <xsd:element name="name" type="DialogElements.WithCondition" minOccurs="0"/>
      <xsd:element name="bundle-id" type="DialogElements" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="SnippetUI.ImageElement.Data">
    <xsd:sequence>
      <xsd:element name="png-data" type="DialogElements" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="SnippetUI.ImageElement.Source">
    <xsd:sequence>
      <xsd:element name="value" minOccurs="0">
        <xsd:complexType>
          <xsd:choice>
            <xsd:element name="data" type="SnippetUI.ImageElement.Data"/>
            <xsd:element name="bundle" type="SnippetUI.ImageElement.Bundle"/>
            <xsd:element name="url" type="SnippetUI.ImageElement.Url"/>
            <xsd:element name="symbol" type="SnippetUI.ImageElement.Symbol"/>
            <xsd:element name="app-icon" type="SnippetUI.ImageElement.AppIcon"/>
          </xsd:choice>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="list" type="xsd:string"/>
    <xsd:attribute name="iterator" type="xsd:string"/>
    <xsd:attribute name="condition" type="xsd:string"/>
  </xsd:complexType>

  <xsd:complexType name="SnippetUI.ImageElement.Symbol">
    <xsd:sequence>
      <xsd:element name="name" type="DialogElements.WithCondition" minOccurs="0"/>
      <xsd:element name="tint-color" type="SnippetUI.Color" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="SnippetUI.ImageElement.Url">
    <xsd:sequence>
      <xsd:element name="source" type="DialogElements" minOccurs="0"/>
      <xsd:element name="content-mode" type="SnippetUI.ImageElement.ContentMode" minOccurs="0"/>
      <xsd:element name="loading_image" minOccurs="0">
        <xsd:complexType>
          <xsd:choice>
            <xsd:element name="data-loading-image" type="SnippetUI.ImageElement.Data"/>
            <xsd:element name="bundle-loading-image" type="SnippetUI.ImageElement.Bundle"/>
            <xsd:element name="symbol-loading-image" type="SnippetUI.ImageElement.Symbol"/>
          </xsd:choice>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="SnippetUI.IntentsUI_Standard">
    <xsd:sequence>
      <xsd:element name="interaction" type="DialogElements" minOccurs="0"/>
      <xsd:element name="slots" type="SnippetUI.IntentsUI_Standard.Slot" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:complexType>

