graphName "FaceTimeAudio_UL_GenericInput"

; -------------------------------------------------------------------------
; Definitions
; -------------------------------------------------------------------------

[def sampleRateIn 44100]
[def numChannelsIn 2]
[def numChannelsOut 1]
[def sampleRateOut 16000]

; -------------------------------------------------------------------------
; Graph Ins and Outs
; -------------------------------------------------------------------------
in in
out out

; -------------------------------------------------------------------------
; Boxes and AUs
; -------------------------------------------------------------------------
box AUChannelSelector ( aufx clsl appl ) 1 1

; -------------------------------------------------------------------------
; DSP Graph topology
; -------------------------------------------------------------------------
wire in AUChannelSelector ([sampleRateIn] [numChannelsIn])
wire AUChannelSelector out ([sampleRateOut] [numChannelsOut])

; -------------------------------------------------------------------------
; Graph parameters and properties
; -------------------------------------------------------------------------

