; =========================================================================
; standard client DSPgraph
; =========================================================================

graphName "BuiltInSpeaker"

; -------------------------------------------------------------------------
; Definitions
; -------------------------------------------------------------------------

[def sampleRateIn 44100]
[def numChannelsIn 2]

; -------------------------------------------------------------------------
; Graph Ins and Outs
; -------------------------------------------------------------------------

in in
out out

; -------------------------------------------------------------------------
; Boxes and AUs
; -------------------------------------------------------------------------

box AUSpeakerProtection (aufx atsp appl) 1 1

; -------------------------------------------------------------------------
; DSP Graph topology
; -------------------------------------------------------------------------

wire in AUSpeakerProtection ([sampleRateIn] [numChannelsIn])

; -------------------------------------------------------------------------
; Graph outputs
; -------------------------------------------------------------------------

wire AUSpeakerProtection out ([sampleRateIn] [numChannelsIn])

; -------------------------------------------------------------------------
; Graph params
; -------------------------------------------------------------------------
