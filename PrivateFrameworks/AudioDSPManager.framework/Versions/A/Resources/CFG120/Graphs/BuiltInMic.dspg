graphName "BuiltInMic"

; -------------------------------------------------------------------------
; Definitions
; -------------------------------------------------------------------------

[def sampleRateIn 44100]
[def numChannelsIn 2]

; -------------------------------------------------------------------------
; Graph Ins and Outs
; -------------------------------------------------------------------------

in in
out out

; -------------------------------------------------------------------------
; Boxes and AUs
; -------------------------------------------------------------------------
box AUChannelSelector (aufx clsl appl) 1 1
box AUFIRFilter (aufx firf appl) 1 1
box AUNBandEQ (aufx nbeq appl) 1 1
box volume dbgn 1 1

; -------------------------------------------------------------------------
; DSP Graph topology
; -------------------------------------------------------------------------
wire in AUChannelSelector ([sampleRateIn] [numChannelsIn])
wire AUChannelSelector AUFIRFilter ([sampleRateIn] [numChannelsIn])
wire AUFIRFilter AUNBandEQ ([sampleRateIn] [numChannelsIn])
wire AUNBandEQ volume ([sampleRateIn] [numChannelsIn])
wire volume out ([sampleRateIn] [numChannelsIn])


; -------------------------------------------------------------------------
; Graph parameters and properties
; -------------------------------------------------------------------------
property slcm in
wireGraphProperty slcm (AUChannelSelector 1936483181 0 0)

param vol0 0
param mut0 0

wireGraphParam vol0 (volume 0 0 0)
wireGraphParam mut0 (volume 1 0 0)
