; =========================================================================
; standard client DSPgraph
; =========================================================================

graphName "VolumeOnly"

; -------------------------------------------------------------------------
; Definitions
; -------------------------------------------------------------------------

[def sampleRateIn 44100]
[def numChannelsIn 2]

; -------------------------------------------------------------------------
; Graph Ins and Outs
; -------------------------------------------------------------------------

in in
out out

; -------------------------------------------------------------------------
; Boxes and AUs
; -------------------------------------------------------------------------

box volume dbgn 1 1

; -------------------------------------------------------------------------
; DSP Graph topology
; -------------------------------------------------------------------------

wire in volume ([sampleRateIn] [numChannelsIn])

; -------------------------------------------------------------------------
; Graph outputs
; -------------------------------------------------------------------------

wire volume out ([sampleRateIn] [numChannelsIn])

; -------------------------------------------------------------------------
; Graph params
; -------------------------------------------------------------------------
param vol0 0
param mut0 0

wireGraphParam vol0 (volume 0 0 0)
wireGraphParam mut0 (volume 1 0 0)
