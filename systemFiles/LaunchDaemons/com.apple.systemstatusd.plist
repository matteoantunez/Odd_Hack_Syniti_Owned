<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.apple.systemstatusd</string>
    <key>MachServices</key>
    <dict>
        <key>com.apple.systemstatus</key>
        <true/>
        <key>com.apple.systemstatus.publisher</key>
        <true/>
        <key>com.apple.systemstatus.activityattribution</key>
        <true/>
    </dict>
    <key>ProgramArguments</key>
    <array>
        <string>/System/Library/PrivateFrameworks/SystemStatusServer.framework/Support/systemstatusd</string>
    </array>
    <key>ProcessType</key>
    <string>Adaptive</string>
    <key>EnablePressuredExit</key>
    <true/>
</dict>
</plist>
